<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rp.androiduicontrols">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextViewClass" />
        <activity android:name=".EditTextClass" />
        <activity android:name=".AutoComplete" />
        <activity android:name=".ButtonClass" />
        <activity android:name=".ImageButtonClass" />
        <activity android:name=".ToggleButtonClass" />
        <activity android:name=".CheckBoxClass" />
        <activity android:name=".RadioButtonClass" />
        <activity android:name=".RadioGroupClass" />
        <activity android:name=".ProgressBar" />
        <activity android:name=".ProgressBarClass" />
        <activity android:name=".SpinnerClass" />
        <activity android:name=".DatePickerClass" />
        <activity android:name=".TimePickerClass" />
        <activity android:name=".SeekBarClass" />
        <activity android:name=".RatingBarClass" />
        <activity android:name=".TextClockClass" />
        <activity android:name=".SwitchClass" />
        <activity android:name=".AlertDialogClass" />
        <activity android:name=".OptionsMenuClass" />
        <activity android:name=".ContextMenuClass" />
        <activity android:name=".PopupMenuClass"></activity>
    </application>

</manifest>